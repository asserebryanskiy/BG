<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="badgegenerator.badgedownload.BadgeDownloadController">
   <children>
      <GridPane hgap="10.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="74.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="58.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="193.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" />
          <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="archiveSizeLabel" text="Вес архива: 124Мб" GridPane.columnSpan="2" GridPane.halignment="CENTER">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Label>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBackToPdfEdit" text="Назад" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
            <Button fx:id="downloadArchiveButton" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#handleDownloadBadges" text="Скачать архив с бейджами" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
      <Rectangle fx:id="progressBarBackground" arcHeight="5.0" arcWidth="5.0" fill="#ffffff7f" height="74.0" stroke="WHITE" strokeType="INSIDE" visible="false" width="281.0" />
      <ProgressBar fx:id="progressBar" prefWidth="200.0" visible="false" />
   </children>
</StackPane>
